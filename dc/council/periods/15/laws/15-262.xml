  <num type="law">15-262</num>
  <heading type="short">Captive Insurance Company Act of 2004</heading>
  <meta>
    <effective>2005-03-17</effective>
    <citations>
      <citation type="law" url="./docs/15-262.pdf">D.C. Law 15-262</citation>
      <citation type="register">52 DCR 1205</citation>
